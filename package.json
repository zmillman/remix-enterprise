{
  "name": "remix-enterprise",
  "version": "0.1.0",
  "type": "module",
  "bin": {
    "remix-enterprise": "bin/remix-enterprise.js"
  },
  "scripts": {
    "dev": "node ./server.js",
    "dev:docker": "docker compose --env-file .env up",
