    "@remix-run/node": "^2.9.1",
    "@remix-run/react": "^2.9.1",
    "aws-cdk-lib": "2.140.0",
    "constructs": "^10.0.0",
    "cross-env": "^7.0.3",
    "express": "^4.19.2",
    "isbot": "^4.4.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "remix-auth": "^3.6.0",
    "remix-auth-google": "^2.0.0",
    "source-map-support": "^0.5.21"
